{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag",
    "description": "Give keywords to your content."
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "webtools": {
      "enabled": true
    }
  },
  "attributes": {
    "label": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "targetField": "label",
      "required": true
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "tags"
    }
  }
}
